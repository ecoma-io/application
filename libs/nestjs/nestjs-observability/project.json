{
  "name": "nestjs-observability",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nestjs/nestjs-observability/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nestjs/nestjs-observability",
        "main": "libs/nestjs/nestjs-observability/src/index.ts",
        "tsConfig": "libs/nestjs/nestjs-observability/tsconfig.lib.json",
        "assets": ["libs/nestjs/nestjs-observability/*.md"],
        "format": ["cjs"],
        "sourcemap": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nestjs/nestjs-observability/jest.config.ts"
      }
    }
  }
}
