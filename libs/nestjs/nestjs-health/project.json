{
  "name": "nestjs-health",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nestjs/nestjs-health/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nestjs/nestjs-health",
        "main": "libs/nestjs/nestjs-health/src/index.ts",
        "tsConfig": "libs/nestjs/nestjs-health/tsconfig.json",
        "assets": ["libs/nestjs/nestjs-health/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nestjs/nestjs-health/jest.config.ts"
      }
    }
  },
  "tags": ["type:lib", "scope:nestjs"]
}
