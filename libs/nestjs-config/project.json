{
  "name": "nestjs-config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nestjs-config/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nestjs-config/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nestjs-config",
        "main": "libs/nestjs-config/src/index.ts",
        "tsConfig": "libs/nestjs-config/tsconfig.lib.json",
        "assets": ["libs/nestjs-config/*.md"]
      }
    },
    "publish": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{workspaceRoot}/dist/{projectRoot}",
        "command": "npm publish --access public"
      }
    }
  }
}
